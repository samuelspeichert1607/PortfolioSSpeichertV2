            <string>//middle of the screen
midx=room_width/2
midy=room_height/2
//set angle for little ship icon (see this object's draw event)
ship_dir=player_1.image_angle

//set coords of player one's ship for little ship icon
if player_1.x&gt;sun.x
{amountx1=player_1.x-sun.x;lpx=midx+amountx1/100}
if player_1.x&lt;sun.x
{amountx1=sun.x-player_1.x;lpx=midx-amountx1/100}
if player_1.y&gt;sun.y
{amounty1=player_1.y-sun.y;lpy=midy+amounty1/100}
if player_1.y&lt;sun.y
{amounty1=sun.y-player_1.y;lpy=midy-amounty1/100}

//set coords of planets for little planet icons
for (n=0; n&lt;global.number_of_planets; n+=1)
{
if planets.x[n]&gt;sun.x 
{amountx=planets.x[n]-sun.x; newpx[n]=midx+amountx/100}
if planets.x[n]&lt;sun.x 
{amountx=sun.x-planets.x[n]; newpx[n]=midx-amountx/100}
if planets.y[n]&gt;sun.y 
{amounty=planets.y[n]-sun.y; newpy[n]=midy+amounty/100}
if planets.y[n]&lt;sun.y 
{amounty=sun.y-planets.y[n]; newpy[n]=midy-amounty/100}
instance_create(newpx[n],newpy[n],lil_planet)
}
//create a new sun object in the screen center (deactivate old one in the lil_sun object)
instance_create(midx,midy,lil_sun)
//deactivate player and planets
instance_deactivate_object(planets)
instance_deactivate_object(player_1)
</string>
