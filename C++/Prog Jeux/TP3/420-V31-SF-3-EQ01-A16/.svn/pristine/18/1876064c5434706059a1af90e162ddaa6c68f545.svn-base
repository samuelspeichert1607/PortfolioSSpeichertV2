#include "Tour.h"

Tour::Tour(bool isAlly)
{
	speed = 0;
	health = 2400;
	maxHealth = 2400;
	range = 0;
	attackPoints = 0;
	cout = 0;
	minionType = MinionType::_Tour;
	this->isAlly = isAlly;
	if (isAlly)
	{
		isFacingLeft = false;
	}
	else
	{
		isFacingLeft = true;
	}
}

Tour::~Tour()
{

}

Minion* Tour::spawn(MinionType type)
{
	return SpawnerMinion::CreateEnnemy(type,getPosition().x,getPosition().y,isAlly);
}