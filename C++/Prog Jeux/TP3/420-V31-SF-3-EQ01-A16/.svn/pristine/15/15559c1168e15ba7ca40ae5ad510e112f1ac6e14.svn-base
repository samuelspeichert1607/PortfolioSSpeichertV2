#include "Barbare.h"

Barbare::Barbare(bool isAlly)
{
	speed = 3;
	health = 600;
	maxHealth = 600;
	range = 5;
	attackPoints = 100;
	cout = 6;
	minionType = MinionType::_Barbare;
	this->isAlly = isAlly;
	if (isAlly)
	{
		isFacingLeft = false;
	}
	else
	{
		isFacingLeft = true;
	}
	Minion();
}

Barbare::~Barbare()
{

}

void Barbare::beserk()
{
	specialAttackTime.restart();
}